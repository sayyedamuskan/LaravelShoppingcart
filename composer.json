{
    "name": "gloudemans/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        }
    ],
    "require": {
        "illuminate/support": "^11.9.2",
        "illuminate/session": "^11.9.2",
        "illuminate/events": "^11.9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3.6",
        "mockery/mockery": "2.0.x-dev",
        "orchestra/testbench":  "^9.4.0"
    },
    "autoload": {
        "psr-4": {
            "Gloudemans\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Gloudemans\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Gloudemans\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
